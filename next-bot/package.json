{
  "name": "telegram-bot",
  "version": "0.1.0",
  "private": true,
  "main": "dist/dev.js",
  "scripts": {
    "lint": "next lint",
    "format": "prettier --check --write \"{**/*,*}.{ts,tsx}\"",
    "webhook:dev": "next dev",
    "webhook:build": "next build",
    "webhook:start": "next start",
    "polling:dev": "concurrently \"pnpm run polling:build\" \"pnpm run polling:start\"",
    "polling:build": "swc src -d dist",
    "polling:start": "nodemon --watch \"dist/**/*\" -e js ./dist/dev.js"
  },
  "dependencies": {
    "@next/font": "13.1.2",
    "grammy": "^1.13.0",
    "next": "13.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.27",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "typescript": "4.9.4"
  }
}
